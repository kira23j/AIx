        "import torch\n",
        "\n",
        "ascii_values = [ord(char) for char in \"Hello, PyTorch!\"]\n",
        "tensor_data = torch.tensor(ascii_values, dtype=torch.int8)\n",
        "hello_string = ''.join([chr(item) for item in tensor_data])\n",
        "print(hello_string)\n"
